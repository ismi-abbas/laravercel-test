{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "outputDirectory": "public",
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.9.0"
        }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_NAME": "Laravercel Test",
        "APP_ENV": "production",
        "APP_DEBUG": "false",
        "APP_URL": "https://laravercel-test.vercel.app",
        "APP_KEY": "base64:f4JlLDxgr1uEa49Zr6/PqNMsokG0hRgo0ui4DVYu3nA=",
        "DB_CONNECTION": "mysql",
        "DB_HOST": "${DB_HOST}",
        "DB_PORT": "${DB_PORT}",
        "DB_DATABASE": "${DB_DATABASE}",
        "DB_USERNAME": "${DB_USERNAME}",
        "DB_PASSWORD": "${DB_PASSWORD}"
    }
}
